.container {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;

  width: 100vw;
  height: 100vh;

  z-index: 1000;

  background-color: rgba(59, 59, 59, 0.78);

  display: flex;
}

.box {
  flex-grow: 1;

  margin: 40px;
  padding: 16px;

  background-color: white;
  border: 1px solid black;
  border-radius: 10px;
}

.dataItem {
  display: flex;
}

.label {
  font-weight: 600;
  width: 100px;
  margin-right: 16px;
  text-align: right;
}

