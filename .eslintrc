{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "no-undef": 0,
    "import/no-extraneous-dependencies": 0,
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "max-len": ["error", 180],
    "no-use-before-define": "off",
    "import/prefer-default-export": "off"
  }
}
